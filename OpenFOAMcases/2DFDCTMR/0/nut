FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.14;

boundaryField
{
    inlet
    {
      type              fixedValue;
	  value             uniform 0.14;
    }

	outlet
	{
	  type              zeroGradient;		
	}

    "(bottom|top)"
    {
      type            fixedValue;
      value           uniform 0;
    }

    "(left|right)"
    {
        type            empty;
    }
}


